{
  "name": "kign.org/codeigniter-email-helper",
  "description": "Email helper for Codeigniter4",
  "minimum-stability": "alpha",
  "license": "proprietary",
  "authors": [
    {
      "name": "bivanbi",
      "email": "bivanbi@kign.org"
    }
  ],
  "repositories": [
    {
